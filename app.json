{
  "expo": {
    "name": "Mobile Flashcards",
    "slug": "mobile-flashcards",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/icon-48.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.remonabeeh.mobileflashcards"
    },
    "android": {
      "package": "io.remonabeeh.mobileflashcards"
    },
    "githubUrl": "https://github.com/remonabeeh/mobile-flashcards"
  }
}
